@*@model IEnumerable<E_Procurement.WebUI.Models.RFQModel.RfqGenModel>*@
<div id="modaldemo6" class="modal fade">
    <div class="modal-dialog modal-dialog-vertical-center" role="document">
        <div class="modal-content bd-0 tx-14">
            <div class="modal-header pd-y-20 pd-x-25">
                <h6 class="tx-14 mg-b-0 tx-uppercase tx-inverse tx-bold">Close RFQ</h6>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>
            <form id="closeRFQId" role="form" asp-controller="RfqGen" asp-action="Close" method="post">
                @Html.AntiForgeryToken()
                @Html.Hidden("RFQId2")
                
                <div class="modal-body pd-25">
                    <div class="form-group col-sm-12">
                        <label class="tx-14 mg-b-0 tx-uppercase tx-inverse tx-bold"><b>Are You Sure You Want To Close This RFQ?</b></label>
                        @*@Html.TextBoxFor(model => model.SingleOrDefault(x => Convert.ToBoolean(x.EndDate)), new { @class = "form-control datepicker", @placeholder = "End Date", @Id = "EndDate", required = "required", type = "date", style = "width:160px; height:40px" })*@

                        @*<input id="EndDate" name="EndDate" class="form-control" type="date" placeholder="Select New Date" required>*@
                    </div>
                  

                </div>
                <div class="modal-footer" style="display:flex; justify-content:space-between">
                    <button type="submit" class="btn btn-info pd-x-20">YES</button>
                    <button type="button" class="btn btn-secondary pd-x-20" data-dismiss="modal">NO</button>
                </div>
            </form>
        </div>
    </div><!-- modal-dialog -->
</div>
